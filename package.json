{
  "name": "sort-es",
  "version": "0.0.8",
  "description": "Blazing fast, tree-shakeable, type-safe, modern utility library to sort any type of array in less than a KB!",
  "scripts": {
    "clean": "rm dist",
    "build": "yarn lint && tsc --project tsconfig.build.json && yarn bundle:esm && yarn bundle:esm:min && yarn bundle:umd && yarn bundle:umd:min && yarn build:stats",
    "build:stats": "(echo '\\033[35;3m' ; cd dist && ls -lh index*js index*gz | tail -n +2 | awk '{print $5,$9}')",
    "bundle:esm": "rollup dist/index.js --file dist/index.mjs --format esm",
    "bundle:esm:min": "terser --ecma 6 --compress --mangle --module -o dist/index.min.mjs -- dist/index.mjs",
    "bundle:umd": "rollup dist/index.js --file dist/index.umd.js --format umd --name sort",
    "bundle:umd:min": "terser --ecma 6 --compress --mangle -o dist/index.umd.min.js -- dist/index.umd.js",
    "cover": "nyc --check-coverage yarn test:only",
    "lint": "eslint src test",
    "_prepublish": "yarn clean && yarn lint && yarn test && yarn build",
    "cypress": ".\\node_modules\\.bin\\cypress open",
    "build-ci": "tsc --project tsconfig.build.json && yarn bundle:esm && yarn bundle:esm:min && yarn bundle:umd && yarn bundle:umd:min && yarn build:stats"
  },
  "main": "dist/index.js",
  "module": "dist/index.min.mjs",
  "unpkg": "dist/index.umd.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosimochellini/sort-es.git"
  },
  "keywords": [
    "sort",
    "sort-es",
    "library",
    "sorting",
    "npm",
    "module",
    "node",
    "javascript",
    "typescript"
  ],
  "author": "Cosimo Chellini <cosimo.chellini@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cosimochellini/sort-es/issues"
  },
  "homepage": "https://github.com/cosimochellini/sort-es#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-eslint": "^10.0.3",
    "cypress": "^4.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.4.0",
    "nyc": "^15.0.0",
    "rollup": "^1.31.1",
    "terser": "^4.6.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {}
}
